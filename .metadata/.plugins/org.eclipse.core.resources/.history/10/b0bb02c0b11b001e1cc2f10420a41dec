package com.ManyToManyhibernatejpa.entity;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.Id;
import jakarta.persistence.Table;
import lombok.NoArgsConstructor;
@Entity
@Table(name="user_course_progress")
public class UserCourseProgress {

	@Id
	@GeneratedValue
	private int id;
	
	private int courseid;
	
	public UserCourseProgress(int id, int courseid, String courseName, int componenetId, int subSectionId,
			String subSection, int subsectionheadid, String subsectionhead, String subsectionDesc, int userid,
			String username, String status, String statusDttime) {
		super();
		this.id = id;
		this.courseid = courseid;
		this.courseName = courseName;
		this.componenetId = componenetId;
		this.subSectionId = subSectionId;
		this.subSection = subSection;
		this.subsectionheadid = subsectionheadid;
		this.subsectionhead = subsectionhead;
		this.subsectionDesc = subsectionDesc;
		this.userid = userid;
		this.username = username;
		this.status = status;
		this.statusDttime = statusDttime;
	}
	public int getCourseid() {
		return courseid;
	}
	public void setCourseid(int courseid) {
		this.courseid = courseid;
	}
	private String courseName;
	private int componenetId;
	private int subSectionId;
	private String subSection;
	private int subsectionheadid;
	private String subsectionhead;
	private String subsectionDesc;
	private int userid;
	private String username;
	private String status;
	private String statusDttime;
	
	public UserCourseProgress() {
		super();
	}
	public UserCourseProgress(int id, String courseName, int componenetId, int subSectionId, String subSection,
			int subsectionheadid, String subsectionhead, String subsectionDesc, int userid, String username,
			String status, String statusDttime) {
		super();
		this.id = id;
		this.courseName = courseName;
		this.componenetId = componenetId;
		this.subSectionId = subSectionId;
		this.subSection = subSection;
		this.subsectionheadid = subsectionheadid;
		this.subsectionhead = subsectionhead;
		this.subsectionDesc = subsectionDesc;
		this.userid = userid;
		this.username = username;
		this.status = status;
		this.statusDttime = statusDttime;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getCourseName() {
		return courseName;
	}
	public void setCourseName(String courseName) {
		this.courseName = courseName;
	}
	public int getComponenetId() {
		return componenetId;
	}
	public void setComponenetId(int componenetId) {
		this.componenetId = componenetId;
	}
	public int getSubSectionId() {
		return subSectionId;
	}
	public void setSubSectionId(int subSectionId) {
		this.subSectionId = subSectionId;
	}
	public String getSubSection() {
		return subSection;
	}
	public void setSubSection(String subSection) {
		this.subSection = subSection;
	}
	public int getSubsectionheadid() {
		return subsectionheadid;
	}
	public void setSubsectionheadid(int subsectionheadid) {
		this.subsectionheadid = subsectionheadid;
	}
	public String getSubsectionhead() {
		return subsectionhead;
	}
	public void setSubsectionhead(String subsectionhead) {
		this.subsectionhead = subsectionhead;
	}
	public String getSubsectionDesc() {
		return subsectionDesc;
	}
	public void setSubsectionDesc(String subsectionDesc) {
		this.subsectionDesc = subsectionDesc;
	}
	public int getUserid() {
		return userid;
	}
	public void setUserid(int userid) {
		this.userid = userid;
	}
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getStatusDttime() {
		return statusDttime;
	}
	public void setStatusDttime(String statusDttime) {
		this.statusDttime = statusDttime;
	}
	
}
